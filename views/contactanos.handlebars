    <section id="main">
        <div id="contact-info-container">
            <h1 class="text-center">
                Bakertilly <br> Honduras
            </h1>
            <div class="contact-info mx-auto">
                <p>
                    Col. Humuya <br> 2ª Calle Sendero Ámbito <br> Edificio OMCAL <br> Tegucigalpa No 2129 <br> Honduras
                    <br> Tel: +504 2239 2663 <br> Fax: +504 9825-5006<br>Email : info@bakertilly.hn
                </p>
            </div>
        </div>
        <div id="form-container">
            <h1>Contáctanos</h1>
            <form action="/contact" method="POST">
                <div class="d-flex" style="justify-content:space-evenly;">
                    <div class="form-group w-50">
                        <input type="text" class="form-control p-4" name="firstName" id="primerNombre"
                            placeholder="Primer Nombre">
                    </div>
                    <div class="form-group w-50 ml-2">
                        <input type="text" class="form-control p-4" name="lastName" id="apellido"
                            placeholder="Apellido">
                    </div>
                </div>
                <div class="d-flex" style="justify-content:space-evenly;">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control p-4" name="email" id="dirección" aria-describedby="emailHelp"
                        placeholder="Dirección">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" name="message" id="mensaje" aria-describedby="emailHelp"
                        placeholder="Escribe tu mensaje">
                </div>
                <div class="d-flex w-100">
                    <button type="submit" class="btn btn-primary mx-auto px-5">Enviar</button>
                </div>
            </form>
        </div>
    </section>
